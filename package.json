{
  "name": "angular-es6-skeleton",
  "version": "1.0.0",
  "description": "A basic Angular 1.5 app skeleton using ES6 and Webpack",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sethbergman/angular-es6-skeleton.git"
  },
  "engines": {
    "node": "5.11.1"
  },
  "homepage": "https://github.com/sethbergman/angular-es6-skeleton",
  "bugs": "https://github.com/sethbergman/angular-es6-skeleton/issues",
  "keywords": [
    "webpack",
    "npm",
    "angular",
    "es6"
  ],
  "author": "Seth Bergman",
  "license": "MIT",
  "dependencies": {
    "angular": "1.7.2",
    "angular-local-storage": "^0.7.1",
    "angular-mocks": "1.7.2",
    "angular-sanitize": "1.7.2",
    "angular-schema-form": "0.8.13",
    "angular-ui-router": "1.0.19",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "css-loader": "~1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-angular": "0.5.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-angular": "3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "file-loader": "~1.1.11",
    "html-webpack-plugin": "~3.2.0",
    "http-server": "^0.11.1",
    "karma": "2.0.4",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "mocha": "~5.2.0",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "~4.9.2",
    "protractor": "^5.4.0",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.0.3",
    "script-loader": "~0.7.2",
    "style-loader": "~0.21.0",
    "susy": "^3.0.5",
    "url-loader": "~1.0.1",
    "webpack": "~4.16.2",
    "webpack-dev-server": "^3.1.2",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "~4.1.3"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf -- node_modules coverage dist",
    "build:dev": "export NODE_ENV=prod && webpack -p",
    "lint": "eslint src/ || true",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --inline --port 5000 --content-base dist/",
    "start:prod": "node server.js --content-base dist/",
    "build": "NODE_ENV=production node node_modules/.bin/webpack && cp src/index.html dist/index.html",
    "test:unit": "karma start",
    "test:unit-watch": "karma start karma.conf.js --auto-watch=true --single-run=False",
    "test:e2e-server": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/webdriver-manager start",
    "test:e2e": "protractor e2e_tests/protractor.config.js --baseUrl='http://0.0.0.0:5000'"
  }
}
