{
  "name": "angular-es6-skeleton",
  "version": "1.0.0",
  "description": "A basic Angular 1.5 app skeleton using ES6 and Webpack",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sethbergman/angular-es6-skeleton.git"
  },
  "homepage": "https://github.com/sethbergman/angular-es6-skeleton",
  "bugs": "https://github.com/sethbergman/angular-es6-skeleton/issues",
  "keywords": [
    "webpack",
    "npm",
    "angular",
    "es6"
  ],
  "author": "Seth Bergman",
  "license": "MIT",
  "dependencies": {
    "angular-mocks": "1.5.8",
    "babel-core": "6.3.26",
    "babel-eslint": "^7.0.0",
    "babel-loader": "6.1.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.1.18",
    "css-loader": "~0.21.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-config-angular": "0.4.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-angular": "0.12.0",
    "eslint-plugin-import": "^2.0.1",
    "expose-loader": "^0.7.1",
    "file-loader": "~0.8.4",
    "html-webpack-plugin": "~1.7.0",
    "karma": "1.2.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "~2.3.0",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "~3.3.3",
    "protractor": "^4.0.10",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.5.2",
    "sass-loader": "~3.0.0",
    "script-loader": "~0.6.1",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.6",
    "webpack": "~1.12.14",
    "webpack-dev-server": "^1.16.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "~0.14.0",
    "angular": "1.5.8",
    "angular-local-storage": "^0.5.0",
    "angular-sanitize": "1.5.8",
    "angular-schema-form": "0.8.13",
    "angular-ui-router": "1.0.0-beta.3",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "susy": "^2.2.12"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf -- node_modules coverage dist",
    "postinstall": "export NODE_ENV=prod && webpack -p",
    "lint": "eslint src/ || true",
    "start:dev": "webpack-dev-server --inline --port 5000 --content-base dist/",
    "start": "node server.js --content-base dist/",
    "test:unit": "karma start",
    "test:unit-watch": "karma start karma.conf.js --auto-watch=true --single-run=False",
    "test:e2e-server": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/webdriver-manager start",
    "test:e2e": "protractor e2e_tests/protractor.config.js --baseUrl='http://0.0.0.0:5000'"
  }
}
